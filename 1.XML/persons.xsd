<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://abis.be/persons" xmlns:base="http://abis.be/base" targetNamespace="http://abis.be/persons" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:import namespace="http://abis.be/base" schemaLocation="base.xsd"/>
	<xs:element name="persons">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tns:person" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="name">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="lastName" type="base:max50Type"/>
				<xs:element name="firstName" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="person">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="tns:name"/>
				<xs:element name="age" type="xs:int"/>
				<xs:element ref="base:country"/>
				<xs:element name="accountNumber" minOccurs="0">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:pattern value="[A-Z]{2}[0-9]{2} [A-Z]{4} \d{10}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="nr" type="xs:int"/>
		</xs:complexType>
	</xs:element>
</xs:schema>